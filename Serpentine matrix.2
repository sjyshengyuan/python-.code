n = int(input())
lists = [[0]*n for i in range(n)]
lists1 = [[0]*n for i in range(n)]
lists[0][0] = 1
for i in range (1,n):
    lists[i][0] = lists[i-1][0] + i
for i in range (n):
    for j in range (1,n):
        lists[i][j] = lists[i][j-1] + i + j + 1
       
for i in range (n):
    if i != 0:
        lists1[i] = [str(j)for j in lists[i][:-i]]
    else:
        lists1[i] = [str(j)for j in lists[i]]
    a = ' '.join(lists1[i])
    print(a)
